{
  "name": "datenkraft/bb-organization-structure-api-php-client",
  "description": "The Organization Structure API PHP Client enables you to work with the Organization Structure API",
  "type": "library",
  "require": {
    "php": "^8.0",
    "datenkraft/bb-base-api-php-client": "^2.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Datenkraft GmbH",
      "email": "office@datenkraft.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Datenkraft\\Backbone\\Client\\OrganizationStructureApi\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "pact-foundation/pact-php": "^7.1",
    "squizlabs/php_codesniffer": "^3.6",
    "jane-php/open-api-3": "^v7.2.5",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
